<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ExtensionManifest ExtensionBundleId="com.example.helloworld" ExtensionBundleName="Hello world" ExtensionBundleVersion="1.0" Version="4.0">
	<ExtensionList>
		<Extension Id="com.example.helloworld.extension" Version="1.0"/>
	</ExtensionList>
	<ExecutionEnvironment>
		<HostList>			
			<Host Name="IDSN" Version="[10.0,20.0]"/>
		</HostList>
		<LocaleList>
			<Locale Code="All"/>
		</LocaleList>
		<RequiredRuntimeList>
			<RequiredRuntime Name="CSXS" Version="7.0"/>
		</RequiredRuntimeList>
	</ExecutionEnvironment>
	<DispatchInfoList>
		<Extension Id="com.example.helloworld.extension">
			<DispatchInfo>
				<Resources>
					<MainPath>./index.html</MainPath>
				</Resources>
				<UI>
					<Type>Panel</Type>
					<Menu>Hello world</Menu>
					<Geometry>
						<Size>
							<Height>400</Height>
							<Width>400</Width>
						</Size>
					</Geometry>
				</UI>
			</DispatchInfo>
		</Extension>
	</DispatchInfoList>
</ExtensionManifest>